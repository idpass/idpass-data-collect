{
  "$schema": "https://typedoc.org/schema.json",
  "name": "ID PASS Data Collect API Documentation",
  "entryPoints": ["src/index.ts"],
  "out": "../../docs/api/datacollect",
  "theme": "default",
  "plugin": [],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "readme": "README.md",
  "includeVersion": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Storage",
    "Sync",
    "Services",
    "Utilities",
    "*"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "titleLink": "https://github.com/idpass/idpass-data-collect",
  "gitRevision": "main",
  "sourceLinkTemplate": "https://github.com/idpass/idpass-data-collect/blob/{gitRevision}/{path}#L{line}",
  "exclude": [
    "**/node_modules/**",
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "externalPattern": [
    "**/node_modules/**"
  ],
  "excludeExternals": true,
  "treatWarningsAsErrors": false,
  "skipErrorChecking": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}