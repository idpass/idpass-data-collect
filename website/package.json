{
  "name": "@idpass/data-collect-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "docusaurus start --port 3001 --host 0.0.0.0",
    "build": "pnpm run generate-api-docs && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy:cloudflare": "wrangler pages deploy build",
    "clear": "docusaurus clear && pnpm run clear-api-docs",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate-api-docs": "node scripts/generate-api-docs.js",
    "generate-postman-collection": "node scripts/generate-postman-collection.js",
    "update-typedoc": "node scripts/update-typedoc.js",
    "clear-api-docs": "rm -rf docs/packages/backend/api-reference-generated && rm -rf docs/packages/datacollect/api"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@mdx-js/react": "3.0.0",
    "clsx": "2.0.0",
    "docusaurus-lunr-search": "^3.6.0",
    "lunr": "^2.3.9",
    "prism-react-renderer": "2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "docusaurus-plugin-typedoc": "^1.4.0",
    "js-yaml": "^4.1.0",
    "openapi-to-postmanv2": "^5.1.0",
    "typedoc": "^0.28.5",
    "typedoc-docusaurus-theme": "^1.4.0",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "~5.6.2",
    "wrangler": "^4.33.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": "22.x"
  }
}